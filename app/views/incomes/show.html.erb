<div class="row">
  <div class="offset-1 col-xs-12">
    <div class="panel panel-border panel-custom">
      <div class="panel-heading">
        <h3 class="panel-title text-center">Receita</h3>
      </div>
      <div class="panel-body">
        <div class="col-sm-12 col-xs-12 col-md-12 col-lg-12 table-responsive">
          <form class="form-horizontal">
            <div class="form-group">
              <label class="col-sm-2 control-label">Ano:</label>
              <div class="col-sm-10">
                <p class="form-control-static"><%= @income.annual_management.year %></p>
              </div>
            </div>
            <div class="form-group">
              <label class="col-sm-2 control-label">Nome:</label>
              <div class="col-sm-10">
                <p class="form-control-static"><%= @income.user_group.first_name %></p>
              </div>
            </div>
            <div class="form-group">
              <label class="col-sm-2 control-label">MÃªs:</label>
              <div class="col-sm-10">
                <p class="form-control-static"><%= @income.month %></p>
              </div>
            </div>
            <div class="form-group">
              <label class="col-sm-2 control-label">Tipo:</label>
              <div class="col-sm-10">
                <p class="form-control-static"><%= @income.kind_desc %></p>
              </div>
            </div>
            <div class="form-group">
              <label class="col-sm-2 control-label">Valor:</label>
              <div class="col-sm-10">
                <p class="form-control-static"><%= @income.income_value %></p>
              </div>
            </div>
            <div class="form-group">
              <div class="col-sm-offset-2 col-sm-9 m-t-15">
                  <%= link_to "Sair", incomes_path, class: "btn btn-white btn-sm waves-effect m-l-5" %>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>

