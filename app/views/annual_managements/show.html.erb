<!-- Start Breadcrumb -->
<% @bread_links = [link_to(t('annual_managements.annual_managements'), annual_managements_path)] %>
<% @current_bread_name = @annual_management.year %>
<!-- End Breadcrumb -->

<div class="row row_frameh">
  <div class="offset-2 col-xs-9">
    <div class="panel panel-border panel-custom">
      <div class="panel-heading">
        <h1 class="panel-title text-center">Ano</h1>
      </div>
      <div class="panel-body">
        <div class="col-sm-12 col-xs-12 col-md-12 col-lg-12 table-responsive">
          <form class="form-horizontal">
            <div class="form-group">
              <label class="col-sm-2 control-label">Ano:</label>
              <div class="col-sm-10">
                <p class="form-control-static"><%= @annual_management.year %></p>
              </div>
            </div>
            <div class="form-group">
              <label class="col-sm-2 control-label">Descrição:</label>
              <div class="col-sm-10">
                <p class="form-control-static"><%= @annual_management.description %></p>
              </div>
            </div>
            <div class="form-group">
              <div class="col-sm-offset-2 col-sm-9 m-t-15">
                  <%= link_to "Sair", annual_managements_path, class: "btn btn-white btn-sm waves-effect m-l-5" %>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>


